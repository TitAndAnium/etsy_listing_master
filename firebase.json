{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "endpoints": {
        "api_getWallet": {
          "region": "us-central1",
          "httpsTrigger": {},
          "invoker": [
            "public"
          ]
        },
        "api_getUserCredits": {
          "region": "us-central1",
          "httpsTrigger": {},
          "invoker": [
            "public"
          ]
        },
        "api_spendCredits": {
          "region": "us-central1",
          "httpsTrigger": {},
          "invoker": [
            "public"
          ]
        },
        "api_createCheckoutSession": {
          "region": "us-central1",
          "httpsTrigger": {},
          "invoker": [
            "public"
          ]
        },
        "stripeWebhook": {
          "region": "us-central1",
          "httpsTrigger": {},
          "invoker": [
            "public"
          ]
        },
        "api_generateListingFromDump": {
          "region": "us-central1",
          "httpsTrigger": {},
          "invoker": [
            "public"
          ]
        },
        "api_generateChainingFromFields": {
          "region": "us-central1",
          "httpsTrigger": {},
          "invoker": [
            "public"
          ]
        },
        "generateFromDumpCore": {
          "region": "us-central1",
          "httpsTrigger": {},
          "invoker": [
            "public"
          ]
        }
      },
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ],
  "hosting": {
    "public": "frontend/dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "index.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-store, no-cache, must-revalidate"
          }
        ]
      },
      {
        "source": "/assets/**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ]
  },
  "firestore": {
    "indexes": "firestore.indexes.json",
    "rules": "firestore.rules"
  },
  "emulators": {
    "hub": {
      "port": 4400
    },
    "ui": {
      "enabled": true,
      "port": 4001
    },
    "functions": {
      "port": 5001,
      "host": "127.0.0.1"
    },
    "auth": {
      "port": 9099,
      "host": "127.0.0.1"
    },
    "firestore": {
      "port": 8089,
      "host": "127.0.0.1"
    },
    "singleProjectMode": true
  }
}
